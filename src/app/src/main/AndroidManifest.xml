<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.capsulo.min808">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <!-- Default Activity -->
        <activity android:name="io.capsulo.min808.features.home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".features.insertnote.InsertNoteActivity" />
        <activity android:name=".features.listnote.ListNoteActivity" />
        <activity android:name=".features.notedetails.NoteDetailsActivity" />
    </application>
</manifest>
